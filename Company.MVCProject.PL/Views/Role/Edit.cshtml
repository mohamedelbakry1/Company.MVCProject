@model RoleToReturnDto
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Role</h1>
<br />
<br />

<form asp-action="Edit" enctype="multipart/form-data">
    <partial name="RolePartialView/_CreateEditPartialView" />


    <div class="my-3">
        <input type="submit" asp-route-id="@Model.Id.ToString()" value="Save" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-light">Back To List</a>
    </div>
</form>
@section ValidationSection {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.min.js"></script>
}


